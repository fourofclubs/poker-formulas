<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<project default="generate-classes">
	<path id="cnphi.dtfs.jooq.classpath">
		<pathelement location="../cnphi-notification" />
		<fileset dir="${cnphi.cnphiHome}\libraries\jOOQ">
			<include name="jooq-3.1.0.jar" />
			<include name="jooq-codegen-3.1.0.jar" />
			<include name="jooq-meta-3.1.0.jar" />
		</fileset>
		<pathelement location="${cnphi.db.driver}" />
		<fileset dir="../cnphi-notification-scala">
			<include name="generator.jar" />
		</fileset>
	</path>
	<target name="generate-classes">
		<java fork="true" classname="org.jooq.util.GenerationTool" classpathref="cnphi.dtfs.jooq.classpath">
			<arg value="/generateConfig.xml" />
		</java>
	</target>
</project>