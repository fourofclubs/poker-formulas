<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE xml>
<configuration xmlns="http://www.jooq.org/xsd/jooq-codegen-3.0.0.xsd">
	<jdbc>
		<driver>oracle.jdbc.driver.OracleDriver</driver>
		<url>jdbc:oracle:thin:@localhost:1521:CNPHI</url>
		<user>CNPHIADMIN</user>
		<password>cnphiadmin</password>
	</jdbc>
	<generator>
		<name>org.jooq.util.DefaultGenerator</name>
		<database>
			<name>org.jooq.util.oracle.OracleDatabase</name>
			<inputSchema>CNPHIADMIN</inputSchema>
			<includes>NOTIF_.*</includes>
		</database>
		<target>
			<packageName>cnphi.notification.generated</packageName>
			<directory>src\main\generated-java</directory>
		</target>
	</generator>
</configuration>
